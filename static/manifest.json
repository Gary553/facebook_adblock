{
  "manifest_version": 2,
  "name": "Ad Blocker for Facebook™",
  "version": "#VERSION#",
  "description": "An open-source Ad Blocker for Facebook™",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "tiratatp",
  "homepage_url": "https://github.com/tiratatp/facebook_adblock",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://*.facebookcorewwwi.onion/*"],
      "css": ["content.css"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
